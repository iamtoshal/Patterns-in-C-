#include <iostream>
#include <stack>
using namespace std;

int main()
{
    stack<int> s;
    s.push(2);

    s.push(12);

    cout << "Size of stack :" << s.size() << endl;

    cout << "Top of the stack :" << s.top() << endl;

    s.pop();

    cout << "Top of the stack : " << s.top() << endl;

    if (s.empty())
    {
        cout << "Stack is empty" << endl;
    }
    else
    {
        cout << "Stack is not empty" << endl;
    }

    return 0;
}
